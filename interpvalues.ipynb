    "import numpy as np\n",
    "\n",
    "def interpolate_vectors(x, y, xnew):\n",
    "    ynew = np.interp(xnew, x, y)\n",
    "    return ynew\n",
    "\n",
    "# Example usage\n",
    "x = [1, 2, 3, 4, 5]\n",
    "y = [10, 15, 7, 12, 8]\n",
    "xnew = [1.5, 2.5, 3.5]\n",
    "\n",
    "ynew = interpolate_vectors(x, y, xnew)\n",
    "print(ynew)\n"
   ]
  }
